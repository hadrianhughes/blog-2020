{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint '*/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "babel-plugin-styled-components": "^2.0.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.28.2",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-styled-components": "^5.3.0",
    "gatsby-source-contentful": "^2.3.46",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^7.1.1",
    "react-responsive": "^8.1.0",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-katex": "^6.0.2",
    "remark-math": "^5.1.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "enzyme-adapter-react-16": "^1.15.4",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.18",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-syntax-highlighter)"
    ]
  }
}
