    "react-responsive": "^8.1.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
